<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:gravity="center" android:layout_width="fill_parent" android:layout_height="fill_parent" android:id="@+id/settingsPageMainLayout">
	<RelativeLayout android:id="@+id/relativeLayout3"
		android:layout_height="fill_parent" android:layout_width="fill_parent"
		android:gravity="top|center_horizontal">
	</RelativeLayout>
	<TableLayout android:id="@+id/tableLayout1"
		android:layout_height="fill_parent" android:layout_width="fill_parent"
		android:stretchColumns="*">
		<TableRow android:layout_width="fill_parent" android:id="@+id/tableRow1"
			android:layout_height="fill_parent">
			<LinearLayout android:orientation="vertical"
				android:layout_width="fill_parent" android:layout_height="fill_parent" android:id="@+id/settingsPageAdLayout">
			</LinearLayout>
		</TableRow>
	</TableLayout>
	<RelativeLayout android:id="@+id/relativeLayoutMainPageBottom"
		android:layout_width="fill_parent" android:layout_height="fill_parent" android:gravity="bottom">
		<LinearLayout android:id="@+id/linearLayout2" android:orientation="vertical" android:layout_height="wrap_content" android:paddingTop="160sp" android:layout_width="fill_parent" android:gravity="left" android:paddingLeft="25sp">
			<TextView android:layout_height="wrap_content" android:text="TextView" android:id="@+id/tvSettingsTitle" android:layout_width="wrap_content"></TextView>
			<LinearLayout android:id="@+id/linearLayout1" android:layout_height="wrap_content" android:layout_width="wrap_content" android:paddingTop="20sp">
				<Button android:layout_height="wrap_content" android:text="Button" android:layout_width="wrap_content" android:id="@+id/btnImperial"></Button>
				<Button android:layout_height="wrap_content" android:text="Button" android:layout_width="wrap_content" android:id="@+id/btnMetric"></Button>
			</LinearLayout>
		</LinearLayout>
		<RelativeLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:gravity="center" android:layout_alignParentBottom="true" android:paddingBottom="12sp" android:id="@+id/settingsLayout">
			<Button android:text="Button" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/btnReturnSettings"></Button>
		</RelativeLayout>
	</RelativeLayout>
</RelativeLayout>
